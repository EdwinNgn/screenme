<h1>Movies#index</h1>
<p>Find me in app/views/movies/index.html.erb</p>
<% console %>
<div class="row">

  <% Movie.top(10).each do |movie| %>
    <% movie_details = movie.get_details %>
    <div class="col-3">
    <%= render "movies/new_movie_card", movie_details: movie_details, movie: movie %>
    </div>
  <% end %>
</div>
