
  <h1>What do you think about those movies?</h1>

  <div class="row justify-content-center align-text">
    <% Movie.top(6).each do |movie| %>
      <div class="col-4 m-3">
        <div class="center">
          <%= render partial: "movies/card_id.html.erb", locals: {omdb_id: movie.omdb_id} %>
        </div>
      </div>
        <%= render partial: "movies/card_collapse.html.erb", locals: {omdb_id: movie.omdb_id} %>
    <% end %>
  </div>


